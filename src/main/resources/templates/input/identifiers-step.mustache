<div class="panel panel-default" {{#if show_optional_toggle}} style='display:none' {{/if}}>
	<div class="panel-heading" role="tab" id="heading-identifiers">
		<div class="panel-title">
			<a id='identifiers-panel-anchor' class='input-accordion-title' role="button" data-toggle="collapse" {{#if is_accordion}} data-parent="#input-page-accordion" {{/if}} href="#collapse-identifiers" aria-expanded="false" aria-controls="collapse-identifiers">
				Identifiers
			</a>
			<span class="green fa fa-check" style="display: none"></span>
            <span class="panelError fa fa-close" style="display: none">&nbsp;ERROR</span>
			<span title="Open" class="pull-right fa fa-chevron-right clickable accordion-toggle-arrow"></span>
		</div>
	</div>
	<div id="collapse-identifiers" class="panel-collapse collapse input-panel-collapse" role="tabpanel" aria-labelledby="heading-identifiers">
		<div class="panel-body">
			<div class="row">
				<div class='col-xs-12'>
					<label id="related-identifiers-lbl" class='control-label'>Related Identifiers</label>{{> tooltips/RelatedIdentifier}}
					<br />
					<br />
					<span class="table-msg"></span>
					<br />
					<br />
					<table id='related-identifiers-data-table' class="table table-condensed table-striped table-bordered">
						<thead>
							<tr>
								<th>Identifier Type</th>
								<th>Relation Type</th>
								<th>Identifier</th>
							</tr>
						</thead>
						<tbody>

						</tbody>
					</table>
				</div>
			</div>
			<div class='row'>
				<div class='col-xs-12 center-text'>
					<button type="button" title="Related Identifiers" id='add-related-identifiers-modal-btn' class="pure-button pure-button-primary btn-lg">Add Related Identifier</button>
				</div>
			</div>
		</div>
	</div>
	<!--Related Identifiers Edit modal-->
	<div class="modal fade" id="related-identifiers-edit-modal" role="dialog">
		<div class="modal-dialog modal-lg">
			<!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4 id="related-identifiers-manage-lbl" class="modal-title">Manage Related Identifiers</h4>
				</div>
				<div class="modal-body dl-horizontal">
					<div id="related-identifiers-modal-error-zone" class='row'>
						<div class='col-xs-12'>
							<div class="error-color">
								<h2 id="related-identifiers-modal-error"></h2>
								<br />
							</div>
						</div>
					</div>
					<div class='row'>
						<div class='col-xs-12'>
							<div class="row">
								<div class="col-xs-12">
									<label id="related-identifier-identifier-type-lbl" for='related-identifier-identifier-type' class='control-label'>Identifier Type</label>{{> tooltips/IdentifierType}}
								</div>
								<div class="col-xs-12">
									<select class='pure-input-1 form-control' title='Identifier Type' id='related-identifier-identifier-type'>
										<option title='' value=''></option>
										<option title='DOI' value='DOI'>DOI</option>
										<option title='URL' value='URL'>URL</option>
									</select>
									<span class="fa fa-check form-control-feedback successCheck" style='display:none'></span>
									<span class="fa fa-times form-control-feedback errorCheck" style='display:none'></span>
								</div>
								<div class="col-xs-12">
									<label id="related-identifier-identifier-type-lbl-error" class="control-label errorCheck">Problem</label>
								</div>
							</div>
							<br />
							<div class="row">
								<div class="col-xs-12">
									<label id="related-identifier-relation-type-lbl" for='related-identifier-relation-type' class='control-label'>Relation Type</label>{{> tooltips/RelationType}}
								</div>
								<div class="col-xs-12">
									<select class='pure-input-1 form-control' title='Relation Type' id='related-identifier-relation-type'>
										<option title='' value=''></option>
										{{#each relation_type}}
											<option title="{{value}}" value="{{value}}">{{value}}</option>
										{{/each}}
									</select>
									<span class="fa fa-check form-control-feedback successCheck" style='display:none'></span>
									<span class="fa fa-times form-control-feedback errorCheck" style='display:none'></span>
								</div>
								<div class="col-xs-12">
									<label id="related-identifier-relation-type-lbl-error" class="control-label errorCheck">Problem</label>
								</div>
							</div>
							<br />
							<div class="row">
								<div class="col-xs-12">
									<label id="related-identifier-identifier-value-lbl" for='related-identifier-identifier-value' class='control-label'>Identifier</label>{{> tooltips/Identifier}}
								</div>
								<div class="col-xs-12">
									<input type='text' id='related-identifier-identifier-value' class='pure-input-1 form-control' title='Identifier' />
									<span class="fa fa-check form-control-feedback successCheck" style='display:none'></span>
									<span class="fa fa-times form-control-feedback errorCheck" style='display:none'></span>
								</div>
								<div class="col-xs-12">
									<label id="related-identifier-identifier-value-lbl-error" class="control-label errorCheck">Problem</label>
								</div>
							</div>
							<br />
						</div>
					</div>
				</div>
				<div class="modal-footer">
					<button type="button" id='related-identifiers-close-btn' class="pure-button">Close</button>&nbsp;&nbsp;
					<span name="delete-btn"><button type="button" id='related-identifiers-delete-btn' title="Delete" class="pure-button button-error">Delete</button>&nbsp;</span>
					<button type="button" id='related-identifiers-save-btn' title="Save and Close" class="pure-button pure-button-primary">Save and Close</button>
				</div>
			</div>
		</div>
	</div>
</div>