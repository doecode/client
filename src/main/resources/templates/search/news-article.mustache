{{#unless has_error}}
      <div class='col-sm-3 hide-xs'>
            <div style='border-bottom: 1px solid #D6D5D4;'>
                  <br />
                  <div class="static-content-title">{{total_results}} Results</div>
                  <br />
            </div>
            <br />
            <span class='search-for-filter-text search-for-filter-header'>Article Types</span>
            <form>
                  <div class="search-sidebar-checkbox-text" style='margin-top:8px;'>
                        {{#each article_types}}
                              <div>
                                    <span>
                                          <label style='font-size:14.45px;'>
                                                <input class='search-checkbox search-article-type' name='article-type-filter' id='article-type-{{art_type}}' value='{{art_type}}' type='checkbox'
                                                      {{#if is_checked}} checked{{/if}} /> <label title='{{value}}' class='search-input-label' for='article-type-{{art_type}}'><span style='display:none;'>text</span></label>&nbsp;{{art_type}}
                                                ({{count}})
                                          </label>
                                    </span>
                              </div>
                        {{/each}}
                  </div>
            </form>
            <br />
            <span class='search-for-filter-text search-for-filter-header'>Publication Date</span>
            <form>
                  <div class="search-sidebar-checkbox-text" style='margin-top:8px;'>
                        {{#each publication_date_years}}
                              <div>
                                    <span>
                                          <label style='font-size:14.45px;'>
                                                <input class='search-checkbox search-publication-year' name='publication-date-filter' id='publication_date-{{pub_year}}' value='{{pub_year}}' type='checkbox'
                                                      {{#if is_checked}} checked{{/if}}/> <label title='{{pub_year}}' class='search-input-label' for='publication_date-{{pub_year}}'><span style='display:none;'>text</span></label>&nbsp;{{pub_year}}
                                          ({{count}})
                                          </label>
                                    </span>
                              </div>
                        {{/each}}
                  </div>
            </form>
      </div>
      <div class='col-sm-9'>
            <!--Right Side Title-->
            <div class='row'>
                  <div class='col-xs-12'>
                        <br />
                        <br />
                        <br />
                  </div>
                  <br />
            </div>
            <div class='row' style='border-left:1px solid #D6D5D4;'>
                  <div class='col-xs-12'>
                        <br />
                        {{#unless hide_featured}}
                              <div class="static-content-title" style='border-bottom:1px solid #D6D5D4;font-size:34px;padding-bottom:15px;'>News<br /></div>
                        {{else}}
                              <div class="static-content-title" style='font-size:34px;padding-bottom:15px;'>News<br /></div>
                        {{/unless}}
                        <br />
                  </div>
                  <br />
            </div>
            <!--Latest article-->
            {{#unless hide_featured}}
                  <div class='row'>
                        <div class='col-xs-12' style='padding-top:20px;border-left:1px solid #D6D5D4;'>
                              <div class='text-muted publication-date'>{{featured_article.publication_date}}</div>
                              <div class='text-muted article-type'>
                                    {{!--Comes out like <icon> Type--}}
                                    {{#if featured_article.article_type.is_news}}
                                          <span class='fa fa-newspaper-o'></span>
                                    {{else if featured_article.article_type.is_updates_and_tips}}
                                          <span class='fa fa-info-circle'></span>
                                    {{else if featured_article.article_type.is_blog}}
                                          <span class='fa fa-file-text-o'></span>
                                    {{/if}}
                                    {{featured_article.article_type.type}}
                              </div>
                              <br />
                              <a href='{{featured_article.url}}' style='font-size:28px;font-weight:300' title='{{featured_article.title}}' target="_blank" rel="noopener noreferrer">{{featured_article.title}}</a>
                              <br />
                              <br />
                              <p style='line-height:30px;'>
                                    {{featured_article.content}}
                                    <br />
                                    <br />
                                    {{#if featured_article.content_over_limit}}
                                          <a href='{{featured_article.url}}' title='{{featured_article.title}}' target="_blank" rel="noopener noreferrer">Read More</a>
                                    {{/if}}
                              </p>
                        </div>
                  </div>
            {{/unless}}
            <!--All of the other articles-->
            <div class='row display-flex' id='article-list'>
                  {{#each refined_articles_list}}
                        <div class='col-md-4 col-sm-6 col-xs-12 news-article-card' data-pubyear='{{publication_year}}' data-articletypelist='{{article_type_str}}'>
                              <span class='text-muted publication-date'>{{publication_date}}</span>
                              <span class='text-muted article-type'>
                                    {{#each article_types}}
                                          {{#if is_news}}
                                                <span class='fa fa-newspaper-o'></span>
                                          {{else if is_updates_and_tips}}
                                                <span class='fa fa-info-circle'></span>
                                          {{else if is_blog}}
                                                <span class='fa fa-file-text-o'></span>
                                          {{/if}}
                                          {{type}}
                                          <br />
                                    {{/each}}
                              </span>
                              {{#need_break}}
                                    <br />
                              {{/need_break}}
                              <br />
                              <br />
                              <div>
                                    <a href='{{url}}' class='article-title' title='{{title}}' target="_blank" rel="noopener noreferrer">{{title}}</a>
                              </div>
                        </div>
                  {{/each}}
            </div>
      </div>
{{else}}
      <div class="col-xs-12 center-text has-error">
            <div class='center-text biblio-title has-error'>
                  <br />
                  <label class='control-label normal-weight'>{{message}}</label>
            </div>
      </div>
{{/unless}}